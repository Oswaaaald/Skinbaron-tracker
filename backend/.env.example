# Environment Configuration
NODE_ENV=development
PORT=8080

# SkinBaron API Configuration
SB_API_KEY=your_skinbaron_api_key_here

# Discord Configuration
DISCORD_WEBHOOK=https://discord.com/api/webhooks/your/webhook/url/here

# Database Configuration
SQLITE_PATH=./data/alerts.db

# Polling Configuration
POLL_CRON=*/2 * * * *
ENABLE_BEST_DEALS=true
ENABLE_NEWEST_ITEMS=true

# Feed Filters
FEEDS_MAX_PRICE=200
FEEDS_MAX_WEAR=0.20

# API Configuration
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# Logging
LOG_LEVEL=info