{
  "name": "skinbaron-alerts-backend",
  "version": "1.0.0",
  "description": "Backend API for SkinBaron Alerts - CS2 skin monitoring and alerting system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "dev:docker": "docker build -f Dockerfile.dev -t skinbaron-backend-dev . && docker run -p 8080:8080 -v $(pwd):/app skinbaron-backend-dev",
    "docker:build": "docker build -t skinbaron-backend .",
    "docker:run": "docker run -p 8080:8080 skinbaron-backend"
  },
  "keywords": [
    "skinbaron",
    "cs2",
    "alerts",
    "discord",
    "fastify",
    "typescript"
  ],
  "author": "SkinBaron Alerts",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^9.6.0",
    "cron": "^3.5.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.6.1",
    "fastify": "^4.29.1",
    "jsonwebtoken": "^9.0.2",
    "undici": "^5.29.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.19.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
