{
  "name": "skinbaron-tracker-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend API for SkinBaron Tracker - CS2 skin monitoring and alerting system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "dev:docker": "docker build -f Dockerfile.dev -t skinbaron-backend-dev . && docker run -p 8080:8080 -v $(pwd):/app skinbaron-backend-dev",
    "docker:build": "docker build -t skinbaron-backend .",
    "docker:run": "docker run -p 8080:8080 skinbaron-backend"
  },
  "keywords": [
    "skinbaron",
    "cs2",
    "alerts",
    "discord",
    "fastify",
    "typescript"
  ],
  "author": "SkinBaron Tracker",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.5",
    "@types/zxcvbn": "^4.4.5",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "cron": "^4.4.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.4",
    "jsonwebtoken": "^9.0.3",
    "lru-cache": "^11.2.5",
    "otplib": "^13.2.1",
    "qrcode": "^1.5.4",
    "undici": "^7.20.0",
    "zod": "^4.3.6",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.0",
    "@types/qrcode": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "pino": "^10.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
